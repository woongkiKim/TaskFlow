{
  "workbench.iconTheme": "material-icon-theme",
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
  },
  "dart.previewFlutterUiGuides": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "[typescript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    // "editor.formatOnSave": true,
  },
  "workbench.settings.useSplitJSON": true,
  "prettier.singleQuote": true,
  "editor.fontFamily": "Ubuntu Mono Nerd Font",
  "editor.tabSize": 2,
  "prettier.trailingComma": "es5",
  "editor.fontSize": 18,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "workbench.startupEditor": "none",
  "editor.wrappingIndent": "indent",
  "editor.wordWrapColumn": 380,
  "terminal.integrated.fontSize": 12,
  "prettier.semi": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
  },
  "terminal.integrated.inheritEnv": false,
  "editor.formatOnSave": true,
  "files.autoSave": "onFocusChange",
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "python": false,
    "dart": true,
    "typescriptreact": true,
    "javascript": true,
    "typescript": true,
    "xml": true,
    "yaml": false,
    "sass": true,
    "json": true,
    "html": false,
    "css": true,
  },
  "workbench.colorCustomizations": {
    // /"terminal.background": "#1A1A1A",
    // "terminal.foreground": "#ffffff",
    // /"terminal.ansiBlack": "#1D2021",
    // "terminal.ansiBrightBlack": "#665C54",
    // "terminal.ansiBrightGreen": "#95C085",
    // /"terminal.ansiBrightMagenta": "#8F4673",
    // /"terminal.ansiBrightRed": "#FB543F",
    // "terminal.ansiBrightWhite": "#FDF4C1",
    // /"terminal.ansiBrightYellow": "#FAC03B",
    // "terminal.ansiGreen": "#C2E88D",
    // /"terminal.ansiMagenta": "#8F4673",
    // /"terminal.ansiRed": "#FB543F",
    // "terminal.ansiWhite": "#A89984",
    /// "terminal.ansiYellow": "#FAC03B"
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "security.workspace.trust.untrustedFiles": "open",
  "application.shellEnvironmentResolutionTimeout": 60,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "workbench.editorAssociations": {
    "*.file": "default",
    "*.pdf": "default",
  },
  "editor.wordWrap": "on",
  "terminal.integrated.fontFamily": "MesloLGS NF",
  "errorLens.enabledDiagnosticLevels": ["error", "warning", "info", "hint"],
  "editor.accessibilitySupport": "off",
  "notebook.lineNumbers": "on",
  "dart.debugExternalPackageLibraries": false,
  "dart.debugSdkLibraries": false,
  "workbench.editor.empty.hint": "hidden",
  "jupyter.askForKernelRestart": false,
  "workbench.colorTheme": "Material Theme Darker",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb, bun.lock",
    "pubspec.yaml": "pubspec.lock,pubspec_overrides.yaml,.packages,.flutter-plugins,.flutter-plugins-dependencies,.metadata",
    "*.dart": "${capture}.g.dart",
    "*.sqlite": "${capture}.${extname}-*",
    "*.db": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.s3db": "${capture}.${extname}-*",
  },
  "github.copilot.nextEditSuggestions.enabled": true,
  "geminicodeassist.updateChannel": "Insiders",
  "workbench.editor.enablePreview": false,
  "workbench.secondarySideBar.defaultVisibility": "hidden",
  "gemini.experimental.mcp": true,
  "chat.mcp.autostart": "onlyNew",
  "chat.mcp.assisted.nuget.enabled": true,
  "diffEditor.codeLens": true,
  "json.schemaDownload.enable": true,
  "dart.openDevTools": "flutter",
  "dart.devToolsLocation": {
    "default": "beside",
    "inspector": "sidebar",
  },
  "python.languageServer": "Default",
}



{
  "workbench.iconTheme": "material-icon-theme",
  "git.autofetch": true,
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "git.confirmSync": false,
  "geminicodeassist.project": "be-music-57ead",
  "geminicodeassist.inlineSuggestions.enableAuto": false,
  "editor.wordWrap": "on",
  "[dart]": {
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": "off"
  },
  "database-client.autoSync": true,
  "workbench.editorAssociations": {
    "*.copilotmd": "vscode.markdown.preview.editor",
    "*.db": "default"
  },
  "diffEditor.experimental.showMoves": true,
  "diffEditor.renderSideBySide": true,
  "workbench.auxiliaryActivityBar.location": "default",
  "editor.unusualLineTerminators": "auto",
  "terminal.explorerKind": "both",
  "terminal.integrated.defaultProfile.windows": "Command Prompt"
}